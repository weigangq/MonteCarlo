        "# birthday problem\n",
        "import random\n",
        "\n",
        "counter = 0\n",
        "for sim in range(1000):\n",
        "  birthdays = []\n",
        "  for n in range(25):\n",
        "    x = random.choice(list(range(0,365)))\n",
        "    if x in birthdays:\n",
        "      counter += 1\n",
        "      break\n",
        "    else:\n",
        "      birthdays.append(x)\n",
        "\n",
        "print((counter/1000)*100)"
